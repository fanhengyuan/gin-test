    "paths": {
        "/font": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Base64 Decode",
                "parameters": [
                    {
                        "type": "string",
                        "description": "base64 string",
                        "name": "base64",
                        "in": "query",
                        "required": true
                    }
                ]
            }
        }
    }
