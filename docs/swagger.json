                },
                "msg": {
                    "type": "string"
                }
            }
        },
        "reportService.ReportStruct": {
            "type": "object",
            "required": [
                "name",
                "phone"
            ],
            "properties": {
                "activity_id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string",
                    "minLength": 1
                },
                "phone": {
                    "type": "string",
                    "minLength": 4
                }
            }
        },
